<table class='table'>
  <thead>
    <tr>
      
      <th>
        image
      </th>
      
      <th>
        name
      </th>
      
      <th colspan="3"></th>
        
    </tr>
  </thead>
  
  <tbady>
    <% users.each do |user| %>
    <tr>
      
      <td>
        <%= image_tag @user.get_profile_image(100,100) %>
      </td>
      
      <td>
        <%= user.name %>
      </td>
      
      <td>
        <%= link_to 'Show', user_path(user.id)%>
      </td>
      
    <% end %>
    </tr>
  </tbady>
</table>